<table>
  <thead>
    <th>Name</th>
    <th>Genre</th>
    <th>Size</th>
  </thead>
  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td>
          <%= game.name %>
        </td>
        <td>
          <%= game.genre %>
        </td>
        <td>
          <%= game.size %>
        </td>
        <td>
          <%= link_to 'Show', game %>
        </td>
        <td>
          <%= link_to 'Edit', edit_gamess_path(game) %>
        </td>
        <td>
          <%= link_to 'Destroy', gamess_path(game), method: :delete %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "Create New Game", new_gamess_path %>