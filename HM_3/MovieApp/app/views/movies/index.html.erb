<table>
  <thead>
    <th>Name</th>
    <th>Time</th>
  </thead>
  <tbody>
    <% @movies.each do |movie| %>
      <tr>
        <td>
          <%= movie.name %>
        </td>
        <td>
          <%= movie.time %>
        </td>
        <td>
          <%= link_to 'Show', movie %>
        </td>
        <td>
          <%= link_to 'Edit', edit_movie_path(movie) %>
        </td>
        <td>
          <%= link_to 'Destroy', movie_path(movie), method: :delete %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "Create New MOvie", new_movie_path %>