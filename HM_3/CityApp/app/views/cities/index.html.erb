<table>
  <thead>
    <th>Name</th>
    <th>Country</th>
    <th>Citizen_count</th>
  </thead>
  <tbody>
    <% @cities.each do |city| %>
      <tr>
        <td>
          <%= city.name %>
        </td>
        <td>
          <%= city.country %>
        </td>
        <td>
          <%= city.citizen_count %>
        </td>
        <td>
          <%= link_to 'Show', city %>
        </td>
        <td>
          <%= link_to 'Edit', edit_city_path(city) %>
        </td>
        <td>
          <%= link_to 'Destroy', city_path(city), method: :delete %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "Create New city", new_city_path %>
